<h1 align="center", id="titles">Contributors</h1>
<div align="center">

	<% @user.each do |user| %>
		<ul class="listing",list-style: none, >
			<div class="row">
				<div class="well col-md-4 col-md-offset-4">

				<li,list-style: none><%= link_to gravatar_for(user), user_path(user) %></li>
					<br>
				<li class="user-title">
				<br>
				<%= link_to user.username, user_path(user) %>

				</li>

				<li, list-style: none><small><%= pluralize(user.articles.count, "article") if user.articles %></small></li>

</div>

</div>

</ul>

<% end %>

</div>